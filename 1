<script>
(function(i,s,o,g,r,a,m){
  i['SnapHeaderConfig']=r;
  a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];
  a.async=1;
  a.src=g;
  m.parentNode.insertBefore(a,m)
})(window,
  document,
  'script',
  'https://storage.googleapis.com/cookie-gdpr-components/snapchatHeader.js',
  {
    domain: 'snapchat.com', // Defaults to snapchat.com
    locale: getLanguage(), // Defaults to en-US. Change to desired locale code.
  }
)
  

  const badLocaleBannerList = ['et', 'ga', 'lv', 'lt', 'mt'];
  const badLocaleBanner = document.querySelector('.bad-locale-header');

  if (badLocaleBannerList.includes(HC.SETTINGS.hcLocale)) {
    console.log('is in bad locale list')
    badLocaleBanner.style.display = 'block';
  } else {
    badLocaleBanner.style.display = 'none';
  }

    

document.addEventListener('DOMContentLoaded', function() {
  const alertBannerVisibility = "false";
  const alertHeaderEle = document.querySelector('.alert-header');
  let height = 0;

  if (alertHeaderEle) {
    height = alertHeaderEle.offsetHeight;
  }
  if (badLocaleBanner) {
    height = badLocaleBanner.offsetHeight;
  }

  if (alertBannerVisibility == 'true' || badLocaleBannerList.includes(HC.SETTINGS.hcLocale)) {
    setTimeout(()=>{
      const snapchatHeader = document.getElementById('snapchat-header-container');
      if (snapchatHeader) snapchatHeader.style.marginTop = `${height}px`;
    }, 500)
  } else {
    setTimeout(()=>{
      const snapchatHeader = document.getElementById('snapchat-header-container');
      if (snapchatHeader) snapchatHeader.style.marginTop = '0px';
    }, 500)
  }
  setTimeout(() => {
    [...document.querySelectorAll('.globalnav-item a')].forEach(ele => {
      if (ele.innerText == 'Support') {
        ele.setAttribute('href', 'https://help.snapchat.com/hc/');
      }
      if (ele.innerText === 'Ads') {
        ele.setAttribute('href', 'https://forbusiness.snapchat.com/?utm_source=help_snapchat_com&utm_medium=referral&utm_campaign=universal_navigation&utm_content=nav_ads');
      }
    })
  }, 800)


  function renderHeaderItems() {
    const globalNavContainer = document.querySelector('.globalnav-container');

    //render home nav item
    function renderHomeHeaderNavItem() {
      const navDiv = document.createElement('div');
      navDiv.classList.add('globalnav-item');
      const navIcon = document.createElement('div');
      navIcon.classList.add('globalnav-icon-home');
      // navIcon.setAttribute('href', HC.SETTINGS.navIcon)
      const navLink = document.createElement('a');
      navLink.classList.add('globalnav-link');
      navLink.setAttribute('href', `/hc/${HC.SETTINGS.hcLocale}`);
      const navText = document.createElement('span')
      navText.innerText = "Home";
      navLink.prepend(navIcon)
      navLink.append(navText)
      navDiv.appendChild(navLink)

      globalNavContainer.appendChild(navDiv)
    }
    
    //render contact us nav item
    function renderContactUsHeaderNavItem () {
      const navDiv = document.createElement('div');
      navDiv.classList.add('globalnav-item');
      const navIcon = document.createElement('div');
      navIcon.classList.add('globalnav-icon-contact-us');
      const navLink = document.createElement('a');
      navLink.classList.add('globalnav-link');
      navLink.setAttribute('href', `/hc/${HC.SETTINGS.hcLocale}/requests/new`);
      const navText = document.createElement('span')
      navText.innerText = "Contact Us";
      navLink.prepend(navIcon)
      navLink.append(navText)
      navDiv.appendChild(navLink)
  
      globalNavContainer.appendChild(navDiv)
    }


    renderHomeHeaderNavItem();

    // render categories nav items
    setTimeout(() => {
      console.log(HC.CATEGORIES)
      if (!HC.CATEGORIES.length) {
        HC.SETTINGS.headerRendered = false;
      }
      Array.from(HC.CATEGORIES).forEach(category => {

        if (category.id != HC.SETTINGS.hiddenCategoryId) {
          const navDiv = document.createElement('div');
          navDiv.classList.add('globalnav-item');

          const navIcon = document.createElement('div');
          navIcon.classList.add(`globalnav-icon-${category.id}`);

          const navLink = document.createElement('a');
          navLink.classList.add('globalnav-link');
          navLink.setAttribute('href', category.html_url);

          const navText = document.createElement('span')
          navText.innerText = category.name;

          navLink.prepend(navIcon)
          navLink.append(navText)
          navDiv.appendChild(navLink)

          globalNavContainer.appendChild(navDiv)

        }

      })
      renderContactUsHeaderNavItem();
      
    }, 500)
    
  }

  HC.renderHeader = () => {    
    if (!HC.SETTINGS.headerRendered) {
      HC.SETTINGS.headerRendered = true;

      setTimeout(() => {
        const hamburgerBtn = document.querySelector('#headerHamburger.header-hamburger');
        const hamburgerContainer = document.querySelector('.header-container')

        if (hamburgerBtn) hamburgerBtn.addEventListener('click', () => {
          let isOpen = '';
    
          if (hamburgerContainer.classList.contains('hamburger-expanded')) {
            isOpen = 'Open'
          } else {
            isOpen = 'Close'
          }
          gtag("event", "hamburger_select", {
            event_label: isOpen,
            event_category: "Menu select",          
          });
    
        })
    
        const ua = navigator.userAgent;

        if (window.innerWidth <= 400) {
          Array.from(document.querySelector('.globalnav-container').children).forEach(child => {
            child.remove();
          })
    

          const headerContainer = document.querySelector('.header-container');
          if (!document.querySelector('.header-nav-link-container')) {
            const headerLinkContainer = document.createElement('div');
            headerLinkContainer.classList.add('header-nav-link-container')
      
            const logoLink = document.createElement('a');
            logoLink.setAttribute('href', `/hc/${HC.SETTINGS.hcLocale}`);
            logoLink.classList.add('logo-nav-container')
      
            const logoIcon = document.createElement('div');
            logoIcon.classList.add('logo-icon');
      
            logoLink.appendChild(logoIcon);
      
            headerLinkContainer.append(logoLink)
      
            const headerText = document.createElement('h2');
            headerText.innerText = 'Snapchat Support'
            headerLinkContainer.append(headerText)
      

            headerContainer.appendChild(headerLinkContainer)
          }

          renderHeaderItems();
        }
        else if (window.innerWidth <= 1070) {
          Array.from(document.querySelector('.globalnav-container').children).forEach(child => {
            child.remove();
          })
    
          const headerContainer = document.querySelector('.header-container');
    
          if (!document.querySelector('.header-nav-link-container')) {
            const headerLinkContainer = document.createElement('div');
            headerLinkContainer.classList.add('header-nav-link-container')
      
            const logoLink = document.createElement('a');
            logoLink.setAttribute('href', `/hc/${HC.SETTINGS.hcLocale}`);
            logoLink.classList.add('logo-nav-container')
      
            const logoIcon = document.createElement('div');
            logoIcon.classList.add('logo-icon');
      
            logoLink.appendChild(logoIcon);
      
            headerLinkContainer.append(logoLink)
      
            const headerText = document.createElement('h2');
            headerText.innerText = 'Snapchat Support'
            headerLinkContainer.append(headerText)
      

            headerContainer.appendChild(headerLinkContainer)
          }
          renderHeaderItems();
          HC.SETTINGS.headerRendered = true;
        }
        showOutageBanner();
      }, 100)
  
    } else {
      const headerContainer = document.querySelector('.header-container');
      if (!document.querySelector('.header-nav-link-container')) {
        const headerLinkContainer = document.createElement('div');
        headerLinkContainer.classList.add('header-nav-link-container')
  
        const logoLink = document.createElement('a');
        logoLink.setAttribute('href', `/hc/${HC.SETTINGS.hcLocale}`);        
        logoLink.classList.add('logo-nav-container')
  
        const logoIcon = document.createElement('div');
        logoIcon.classList.add('logo-icon');
  
        logoLink.appendChild(logoIcon);
  
        headerLinkContainer.append(logoLink)
  
        const headerText = document.createElement('h2');
        headerText.innerText = 'Snapchat Support'
        headerLinkContainer.append(headerText)
  

        headerContainer.appendChild(headerLinkContainer)
      }
    }
  }

  HC.renderHeader();

});


</script>



