document.addEventListener('DOMContentLoaded', function() {



  function makeDropdownFromJSON(jsonarr) {
    const selectEle = document.createElement('select');
    selectEle.setAttribute('id', 'dropdown')
    selectEle.style.display = 'block';

    jsonarr.forEach(pair => {
      const optionEle = document.createElement('option');
      optionEle.value = pair.value;
      optionEle.text = pair.label;
      selectEle.add(optionEle);
    })

    return selectEle;
  }

    

  const hint = document.querySelector('#request_description_hint');
  if (hint) {
    hint.style.display = 'none';
  }

  const anonEmailEle = document.querySelector('.request_anonymous_requester_email');
  if (anonEmailEle) {
    anonEmailEle.style.display = 'none';
  }

  function addMetaDescription() {
    const metaTag = document.createElement('meta');
    metaTag.name = 'description';
    metaTag.content = 'The Snapchat Help Center has support articles and additional resources so you can learn how to use Snapchat product features and resolve technical issues. Still need help? Contact Us.';
    document.head.appendChild(metaTag);
  }

  addMetaDescription();

  function changeDescription() {
    if (window.location.href.includes('ticket_form_id')) {
      const urlparams = new URLSearchParams(window.location.search);
      if (urlparams.get('ticket_form_id') == '11457592603924') {
        console.log('got it')
        const descLabel = document.querySelector('#request_description_label');
        if (descLabel) {
          descLabel.innerText = "Please provide more details here on the reason for your appeal."
        }
      }
    }
  }

  changeDescription();

  
