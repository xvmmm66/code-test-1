                <div v-if="hasLv3 && hasLv2Blocking === false && ch.kind == 'question'">
                  <p>{[{getDCInput(ch)}]}</p>
                  <div class="cuf-questions-list">
                    <div v-for="c in ch._children">
                      <input :type="ch.type" :id="c.key" :value="c.text" name="third-question" @click="saveThirdInput(c)" :checked="c.selected === true">
                      <label :for="c.key">{[{getDCInput(c)}]}</label>
                    </div>
                  </div>
                </div>
                
              </template>  
            </template>
          </div>

          <div v-if="thirdAnswer._children" class="cuf-followup cuf-followup-3">
