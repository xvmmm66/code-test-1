<script type="text/javascript">
  let HC = window.HC || {};

  HC.formMappings = {}
  HC.mappingsForm = {}

  HC.ARTICLES = [];
  HC.SECTIONS = [];
  HC.CATEGORIES = [];
  HC.HIDDENCAT = {};

  HC = {
    formMappings: {},
    mappingsForm: {},

    ARTICLES: [],
    SECTIONS: [],
    CATEGORIES: [],
    HIDDENCAT: {},

    CUF: cufData[0]
  }

  function formSubmit(recaptchaResponse) {
    const snapAPIUrl = "https://wassupsnap.appspot.com/en-US/api/v2/zendesk/send";
    let snapLocale = HC.SETTINGS.snapLocale;

    if (snapLocale == 'ar') snapLocale = 'ar-aa';
    if (snapLocale == 'da') snapLocale = 'da-DK';
    if (snapLocale == 'de') snapLocale = 'de-DE';
    if (snapLocale == 'fi') snapLocale = 'fi-FI';
    if (snapLocale == 'it') snapLocale = 'it-IT';
    if (snapLocale == 'ja') snapLocale = 'ja-JP';
    if (snapLocale == 'nl') snapLocale = 'nl-NL';
    if (snapLocale == 'no') snapLocale = 'nb-NO';
    if (snapLocale == 'sv') snapLocale = 'sv-SE';
    if (snapLocale == 'ro') snapLocale = 'ro-RO';
    if (snapLocale == 'ru') snapLocale = 'ru-RU';
    if (snapLocale == 'tr') snapLocale = 'tr-TR';
    if (snapLocale == 'hi-in') snapLocale = 'hi-IN';
    if (snapLocale == 'pt-pt') snapLocale = 'pt-PT';
    if (snapLocale == 'el') snapLocale = 'el-GR';
    if (!snapLocale) snapLocale = 'en-US';

    console.log('snap locale to send to API', snapLocale);

    const currentLocaleSnapAPIUrl = snapAPIUrl.replace(/com\/([A-Z]{2})([-]([A-Z]{2}))?/i, `com/${snapLocale}`);
    console.log('API URL: ', currentLocaleSnapAPIUrl)
    zdRequestForm().formSubmit(currentLocaleSnapAPIUrl, recaptchaResponse);
  }

  function captchaCallback() {
    const formFooter = document.querySelector('#new_request footer');
    if (formFooter) {
      const submitButton = formFooter.querySelector('input');
      const recaptchaElement = document.createElement('div');
      recaptchaElement.classList.add('g-recaptcha');
      recaptchaElement.style.marginBottom = '10px';
      recaptchaElement.style.marginTop = '30px';

      formFooter.insertBefore(recaptchaElement, submitButton);


      grecaptcha.render(recaptchaElement, {
        'sitekey'  : "6Ldt4CkUAAAAAJuBNvKkEcx7OcZFLfrn9cMkrXR8",
        'size': 'invisible',
        'callback' : formSubmit,
      });

      // Moving the recaptcha above the submit button on mobile only
      if (window.innerWidth < 768 && window.location.href.includes('ticket_form_id')) {
        var recaptcha = document.querySelector('.g-recaptcha');
        var requestFormFooter = document.querySelector('#new_request footer');
        requestFormFooter.parentNode.insertBefore(recaptcha, requestFormFooter);   
      }
    } else {
      console.log('captcha callback: no form footer')
    }
  }


  async function getForms() {
    const response = await fetch('/api/v2/ticket_forms');
    const data = await response.json();
    data.ticket_forms.forEach(form => {
      HC.formMappings[form.name.toLowerCase().replaceAll(' ', '-').replaceAll('-form', '')] = {
        id: form.id
      }
    })
    data.ticket_forms.forEach(form => {
      HC.mappingsForm[form.id] = form.name;
    })    

    HC.formMappings['report-trademark'] = HC.formMappings['trademark-infringement'];
    delete HC.formMappings['trademark-infringement'];

    HC.formMappings['report-copyright'] = HC.formMappings['copyright'];
    delete HC.formMappings['copyright'];

    HC.formMappings['information-privacy'] = HC.formMappings['information-privacy-questions'];
    delete HC.formMappings['information-privacy-questions'];

    HC.formMappings['privacy-rights'] = HC.formMappings['[legal]-privacy-rights-request-submission'];
    delete HC.formMappings['[legal]-privacy-rights-request-submission'];

    Object.keys(HC.formMappings).forEach(key => {
      const formID = HC.formMappings[key].id;
      if (key.toLowerCase() !== HC.mappingsForm[formID].toLowerCase()) {
        HC.mappingsForm[formID] = key;
      }
    })
  }

  getForms();

  const guideToGCPLocale = {
    'ar': "ar",
    "da": "da-DK",
    "de": "de-DE",
    "en-gb": "en-GB",
    "en-us": "en-US",
    "es": "es",
