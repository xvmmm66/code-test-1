  HC.hiddenForms = hiddenForms;

  // check the which form 
  function checkTicketForm () {
    if (window.location.href.includes('ticket_form_id')) {
      let path = window.location.href;
      if (path.includes('&co=true')) {
        path = path.replace('&co=true', '')
      }

      const links = path.split('/'),
            page = links[links.length - 1],
            resultArr = page.split('ticket_form_id='),
            result = Number(resultArr[resultArr.length - 1])

      return result
    }
  }

  // Checks if it is a hidden form and hides the default Flow and just showers the form
  function checkForHiddenForms() {
    var tContactUs = "Submit a request";
    const urlParams = new URLSearchParams(window.location.search);
    const formID = urlParams.get('ticket_form_id');
    if (window.location.href.includes('co=true')) {
      console.log(hiddenForms[formID])      
      if (hiddenForms[formID]) {
        document.querySelector('#new_request').style.display = 'block';
        document.querySelector('#v-contact-us-flow').style.display = 'none';

        const header = document.querySelector('.content-header');
        const dcSnippet = document.querySelector('.dc-snippet');
        dcSnippet.style.display = 'block'
        header.innerHTML = hiddenForms[formID]?.title ? hiddenForms[formID]?.title : tContactUs;
        dcSnippet.innerHTML = hiddenForms[formID]?.snippet ? hiddenForms[formID]?.snippet : '';
        if (dcSnippet.innerText == '') {
          dcSnippet.style.display = 'none';
        }

        const breadcrumbs = document.querySelector('.breadcrumbs');
        breadcrumbs.style.display = 'none';

        const metaEle = document.createElement('meta');
        metaEle.name = 'robots';
        metaEle.content = "noindex";
        document.head.appendChild(metaEle);

        if(window.location.href.includes('17471023300884')){          
          const formField1 = document.querySelector('.request_custom_fields_23699468763412');
          const label1 = document.querySelector('#request_custom_fields_23699468763412_label')
          const section1 = document.createElement('p');
          section1.innerHTML = `Tell us about yourself`
          section1.style.fontWeight = 'bolder'
          section1.style.fontSize = '15px'
          section1.style.textDecoration = 'underline'
          formField1.insertBefore(section1,label1)

          const formField2 = document.querySelector('.request_custom_fields_23701027754644')
          const label2 = document.querySelector('#request_custom_fields_23701027754644_label')
          const section2 = document.createElement('p')
          section2.innerHTML = `Tell us about what you’d like to report`
          section2.style.fontWeight = 'bolder'
          section2.style.fontSize = '15px'
          section2.style.textDecoration = 'underline'
          formField2.insertBefore(section2,label2)

          const email = document.querySelector('#request_custom_fields_24335325_label')
          email.innerHTML = `Your email`

          const username = document.querySelector('#request_custom_fields_24281229_label')
          username.innerHTML = `Your username (or username of account reporting on behalf of)`
          
          const description = document.querySelector('#request_description_label')
          description.innerHTML = `Description of the violation (Please provide a link or ID of the violating content, if possible)`
        }

        if (window.location.href.includes('16872133077140')) {
          const form = document.querySelector('.request-form');
          const fullNameNode = document.querySelector('.request_custom_fields_17383165396756');
          console.log(fullNameNode);
          const footerNode = document.querySelector('.request-form footer');
          const descNode = document.querySelector('#request_description_label');

          const dcNotAccountOwner = document.createElement('p');
          dcNotAccountOwner.innerHTML = dynamicContents.snapStarNotAccountOwner;
          const dcAgreement = document.createElement('span');
          dcAgreement.innerHTML = dynamicContents.snapStarFormSubmissionAgreement;
          descNode.innerHTML = dynamicContents.snapStarDescTitle;

          footerNode.style.margin = 0;
          footerNode.style.padding = 0;
          
          form.insertBefore(dcNotAccountOwner, fullNameNode)
          form.insertBefore(dcAgreement, footerNode)
        }

        // hidden form - illegal content form
        if (window.location.href.includes('17470990658068')) {
          const emailFieldHelperEle = document.querySelector('#request_custom_fields_24335325_hint');
          emailFieldHelperEle.innerHTML = dynamicContents.illegalContentEmailDesc;

          const descFieldEle = document.querySelector('#request_description_label');
          descFieldEle.innerHTML = dynamicContents.illegalContentDesc;
        }

        if (window.location.href.includes('19326996115732')) {
          //replace username
          const usernameText = document.querySelector('#request_custom_fields_24281229_label');
          usernameText.innerHTML = `Snap Star Username`

          //replace email
          const emailText = document.querySelector('#request_custom_fields_24335325_label');
          emailText.innerHTML = `Email associated with creator’s Snapchat account`

          //replace description text of email
          const emailDescText = document.querySelector('#request_custom_fields_24335325_hint')
          emailDescText.innerHTML = `Please Note: We cannot take further action on your ticket without this information`

          //add additional text
          const relationshipToCreatorFieldEle = document.querySelector('.request-form');
          const relationshipToCreatorText = document.querySelector('.request_custom_fields_16881130695956');
          const newP = document.createElement('p');
          newP.innerHTML = `<p>If you're <strong>not</strong> the owner of this Snapchat account, please input your information below.</p>
`
          relationshipToCreatorFieldEle.insertBefore(newP, relationshipToCreatorText)

          //replace description
          const descText = document.querySelector('#request_description_label');
          descText.innerHTML = `Detailed description of the issue`
        }

        // if twitter esc
        if (window.location.href.includes('20269107174548')) {
          const relationshipToCreatorFieldEle = document.querySelector('.request-form');
          const emailField = document.querySelector('.request_custom_fields_24335325');
          const newP = document.createElement('p');
          newP.innerHTML = `<strong>Please Note</strong>: We cannot take further action on your ticket without this information.`
          relationshipToCreatorFieldEle.insertBefore(newP, emailField.nextSibling)

          const descText = document.querySelector('#request_description_label');
          descText.innerHTML = `Detailed description of the issue`
        }

        if (window.location.href.includes('20182040055060') && HelpCenter.user.role != 'anonymous') {
          document.querySelector('.request_custom_fields_20520147873556').style.display = 'block'
        }

        if (window.location.href.includes('17471023300884')) {
          // add text
          const whoAreYouReportingLabel = document.querySelector('#request_custom_fields_23699468763412_label');
          const fieldContainer = document.querySelector('.request_custom_fields_23699468763412');
          const newP = document.createElement('p');
          newP.innerHTML = `<p>When you report something, your information is not shared with the person whose content and/or account you're reporting.</p>
<hr>
<br>`
          fieldContainer.insertBefore(newP, whoAreYouReportingLabel)

          // changed age field label text
          const yourAgeLabel = document.querySelector('#request_custom_fields_24389845_label');
          yourAgeLabel.innerHTML = `Your age (or age of account reporting on behalf of)`

          // add helper text
          const whereDidYouSeeThisEle = document.querySelector('.request_custom_fields_23701070510228');
          const newP2 = document.createElement('p');
          newP2.innerHTML = `<p>
  To review the different spaces in the Snapchat app, review
  <a href="https://help.snapchat.com/hc/articles/7012399221652">this article</a>.
</p>`
          whereDidYouSeeThisEle.appendChild(newP2)

          const usernameReportedAccsEle = document.querySelector('.request_custom_fields_24335443');
          const newP3 = document.createElement('p');
          newP3.innerHTML = `<p>
  To learn how to identify usernames on Snapchat, watch
  <a href="https://videos.ctfassets.net/kw9k15zxztrs/38fQaV55yQcI8CRFxM3fvF/26c18e6273b24102989a97826b89b37b/20230118_SNAP_UsernameVideo.mp4">this video</a>.
</p>`
          usernameReportedAccsEle.appendChild(newP3)

          const descEle = document.querySelector('.request_description');
          const newP4 = document.createElement('p');
          newP4.innerHTML = `In order for us to help you, please try to be as detailed as possible. If you are reporting a Story, please include the link to the Story by pressing and holding on the Story, tapping “Copy link," and pasting the link in your report. If possible, please report the harmful content or user in the Snapchat app, in addition to submitting this form. This will help us provide a better response.`
          descEle.appendChild(newP4)
        }


      } else {
        window.location.replace('/hc/requests/new')
      }
      
    }
  }

  if (window.location.href.includes('ticket_form_id')) checkForHiddenForms()

  // New request form customizations
  let hiddenFormsList = `360001202252,228863,360000014503,134363,224203,247803,360001266791,360001234011,360001052692,101966,360001156631,360000742991,360000999372,360000106503,211703,71659,360000005946,4415382039828,4415368384788`
  var alertBannerVisibility = "false";
  if (alertBannerVisibility == 'true') {
    document.querySelector('.cuf-hero').style.marginTop = '30px';
  } 
  let newRequestFormConfig = {
    hiddenFormsList: hiddenFormsList.split(','),
  }

  zdRequestForm().init(newRequestFormConfig);

});

